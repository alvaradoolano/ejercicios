Algoritmo regaloparael14defebreroJA
	Definir presupuesto Como Real
	//datos de entrada
	Escribir "ingrese el valor del presupuesto:";
	Leer presupuesto;
	//Proceso 
	si presupuesto<=10 Entonces
		Escribir "tarjeta";
	FinSi
	si presupuesto>=11 y presupuesto<=100 Entonces
		Escribir "chocolates";
	FinSi
	si presupuesto>=101 y presupuesto<=250 Entonces
		Escribir "flores";
	FinSi
	si presupuesto>=251 Entonces
		Escribir "anillo";
	FinSi
	//datos de salida
	Escribir "elpresupuestoes:",presupuesto

	
	
	
	
FinAlgoritmo
